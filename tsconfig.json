{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@i18n/*": ["src/i18n/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@styles/*": ["src/styles/*"],
      "@js/*": ["src/js/*"],
      "@content/*": ["src/content/*"],
      "@icons/*": ["src/components/icons/*"]
    }
  }
}
